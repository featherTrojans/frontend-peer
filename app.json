{
  "expo": {
    "name": "Feather",
    "slug": "Feather",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "notification": {
      "icon": "./assets/images/notification.png",
      "iosDisplayInForeground": true
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/resplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003AD6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "africa.feathermobile.com",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Feather needs to use Face ID to authenticate"
        
      
      }
    },
    "android": {
      "versionCode": 11,
      "package": "feather.peer",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE",
        "MANAGE_DOCUMENTS",
  
        "READ_INTERNAL_STORAGE",
        "MODIFY_AUDIO_SETTINGS",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_WIFI_STATE",
        "WRITE_SETTINGS",
        "READ_CONTACTS",
        "CAMERA",
        "VIBRATE",
        "SYSTEM_ALERT_WINDOW",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "RECORD_AUDIO",
        "USE_BIOMETRICS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBzIYA7wqNVTxq0LTO88-cIue_X9mXd5-w"
        }
      }
    },
    "web": {
      "favicon": "./assets/images/notification.png"
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "5daafb00-ef60-44d8-92f2-a196e80c243c"
      }
    }
  }
}
