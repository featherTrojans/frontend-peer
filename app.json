{
  "expo": {
    "name": "Feather",
    "slug": "Feather",
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "notification": {
      "icon": "./assets/images/notification.png",
      "iosDisplayInForeground": true
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/resplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#003AD6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": false,
      "url": "https://u.expo.dev/93d5876e-2546-4e40-ad80-4fe20b9d48fa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.feather.peer",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSContactsUsageDescription": "Allow Feather access to your contacts to enable you see which of your contacts also uses feather, so you can chat them and perform cash transactions with them",
        "NSLocationUsageDescription": "Allow Feather access to your location to find the nearest agent close to you"
      }
    },
    "android": {
      "versionCode": 13,
      "package": "feather.peer",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE",
        "MANAGE_DOCUMENTS",
        "READ_INTERNAL_STORAGE",
        "MODIFY_AUDIO_SETTINGS",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_WIFI_STATE",
        "WRITE_SETTINGS",
        "READ_CONTACTS",
        "CAMERA",
        "VIBRATE",
        "SYSTEM_ALERT_WINDOW",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "RECORD_AUDIO",
        "USE_BIOMETRICS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/notification.png"
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "93d5876e-2546-4e40-ad80-4fe20b9d48fa"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
